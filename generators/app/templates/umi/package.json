{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8000 PROXY_TARGET=mock umi dev",
    "build": "umi build",
    "test": "umi test",
    "lint": "eslint --ext .js src mock tests",
    "docker:build": "./docker/scripts/docker.build.sh"
  },
  "dependencies": {
    "antd": "^3.25.2",
    "cool-utils": "~0.0.3",
    "dva": "^2.6.0-beta.6",
    "lodash-es": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^2.12.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "commitizen": "^4.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-cool": "^0.3.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "prettier-eslint": "^9.0.1",
    "react-test-renderer": "^16.12.0",
    "umi-plugin-react": "^1.14.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "worker-loader": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,sass,scss,less,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
