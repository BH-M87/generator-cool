{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "cool-cli build",
    "precommit": "lint-staged",
    "start": "cool-cli start",
    "publish:predaily": "tnpm i -g @ali/def-pub-client",
    "publish:daily": "dps -d",
    "publish:java": "./publish.sh",
    "build:localify": "npm run build && onebox-localify --scan-path ./build --backend-path /"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,sass,scss,less,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "commitizen": "^3.0.5",
    "cool-cli": "^1.0.1",
    "eslint": "^5.11.0",
    "eslint-config-cool": "~0.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.13.7"
  },
  "dependencies": {
    "antd": "^3.10.7",
    "classnames": "^2.2.5",
    "dva": "^2.4.1",
    "dva-loading": "^2.0.6",
    "haversine-distance": "^1.1.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "prop-types": "^15.7.2",
    "rc-queue-anim": "^1.3.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-image": "^1.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-visibility-sensor": "^5.0.2",
    "url-search-params": "^1.1.0",
    "uuid": "^3.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
